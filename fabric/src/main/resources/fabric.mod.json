{
  "schemaVersion": 1,
  "id": "json-model-extensions",
  "version": "${version}",

  "name": "JSON Model Extensions",
  "description": "JSON Model Extensions for Fabric Rendering",
  "authors": [
    "Grondag", "alex5nader"
  ],
  "contact": {
    "sources": "https://github.com/grondag/json-model-extensions",
    "homepage": "https://github.com/grondag/json-model-extensions",
    "issues": "https://github.com/grondag/json-model-extensions/issues"
  },
  "license": "Apache-2.0",
  "icon": "assets/jmx/jmx_icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [ "grondag.jmx.JsonModelExtensions" ],
    "frex": [ "grondag.jmx.target.FrexHelper" ]
  },
  "mixins": [
	    "jmx.mixins.json"
  ],
  "custom": {
    "modmenu": {
        "badges": [ "library" ]
    }
  },
  "depends": {
    "fabricloader": ">=0.12.5",
    "minecraft": "1.17.1",
    "java": ">=16",
    "fabric": "*"
  }
}
