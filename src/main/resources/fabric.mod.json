{
  "schemaVersion": 1,
  "id": "jmx",
  "version": "${version}",
  "name": "JSON Model Extensions",
  "description": "JSON Model Extensions for Minecraft Rendering",
  "authors": [
    "Grondag", "alex5nader", "mo0dss"
  ],
  "contact": {
    "sources": "https://github.com/vram-guild/json-model-extensions",
    "homepage": "https://github.com/vram-guild/json-model-extensions",
    "issues": "https://github.com/vram-guild/json-model-extensions/issues"
  },
  "license": "LGPL-3.0",
  "icon": "assets/jmx/jmx_icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "io.vram.jmx.client.JsonModelExtensions"
    ]
  },
  "custom": {
    "modmenu": {
      "badges": [ "library" ]
    }
  },
  "mixins": [
    "jmx.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
